docker run \
    --name myPostgresDb \
    -p 5432:5432 \
    -e POSTGRES_USER=postgresUser \
    -e POSTGRES_PASSWORD=postgresPassword \
    -e POSTGRES_DB=postgres \
    -d \
    postgres
docker cp init.sql myPostgresDb:/
sleep 1
docker exec -it myPostgresDb psql -U postgresUser -d postgres -f /init.sql
